    "## Print the required output in given format\n",
    "n = int(input())\n",
    "i = 1\n",
    "while i<=n :\n",
    "    j = 1\n",
    "    k = i\n",
    "    while j <= i:\n",
    "        print(k, end = \"\")\n",
    "        j = j + 1\n",
    "        k = k-1\n",
    "    print()    \n",
    "    i = i+1"
