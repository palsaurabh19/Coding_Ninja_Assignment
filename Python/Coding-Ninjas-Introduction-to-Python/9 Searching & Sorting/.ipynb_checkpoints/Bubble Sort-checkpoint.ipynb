    "from sys import stdin\n",
    "def bubbleSort(arr, n) :\n",
    "    for i in range(n-1):\n",
    "        for j in range(n-i-1):\n",
    "            if arr[j+1] < arr[j]:\n",
    "                arr[j+1],arr[j] = arr[j],arr[j+1]\n",
    "    return arr"
   ]
  }
